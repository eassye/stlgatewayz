@section scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            $('#calendar').fullCalendar({
                theme: true,
                header: {
                    left: 'prev,next today',
                    center: 'title',
                    right: 'month,agendaWeel,agendaDay'
                },

                defaultView: 'agendaDay',
                editable: true,
                allDaySlot: false,
                selecttable: true,
                slotMinutes: 15,
                events: "/home/getevents/"
            });
        });
    </script>
}


<div class="jumbotron">
        <hi>Full Calendar Demo</hi>
</div>

<div class="row">
        <div class="col-md-12">
            <div id="calendar"></div>
        </div>
</div>